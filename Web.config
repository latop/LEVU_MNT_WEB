<?xml version="1.0" encoding="utf-8"?>
<!-- PRD -->
<configuration>
  <configSections>
    <sectionGroup name="elmah">
      <section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
      <section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
    </sectionGroup>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SIGLA.Web.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <elmah>
    <security allowRemoteAccess="1" />
    <!--
            Use to log errors into separate XML files that are stored on 
            disk at the path specified in the logPath attribute.
        -->
    <errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/Log" />
  </elmah>
  <appSettings>
    <!--
		PARÂMETROS PARA ENVIO DE E-MAIL DE SOLICITAÇÃO DE PASSE
		Os valores dos parâmetros são obtidos da tabela sig_parametros.
		Se os valores desses parâmetros estiverem vazios na tabela sig_parametros,
		o sistema tentará obtê-los através das configurações abaixo.
	-->
    <!-- localhost -->
    <add key="SmtpServer" value="" />
    <add key="SmtpServerLogin" value="" />
    <add key="SmtpServerSenha" value="" />
    <!-- ReenvioSenhaEmailFrom só pode ter 1 endereço de e-mail -->
    <add key="ReenvioSenhaEmailFrom" value="" />
    <!-- SolicitacaoPasseEmailFrom só pode ter 1 endereço de e-mail -->
    <add key="SolicitacaoPasseEmailFrom" value="" />
    <!-- SolicitacaoPasseEmailTo pode ter 1 ou mais endereços de e-mail separados por ; -->
    <add key="SolicitacaoPasseEmailTo" value="" />
    <add key="ChartImageHandler" value="Storage=file;Timeout=20;Url=~/TempImages/;deleteAfterServicing=false;" />
    <!-- <add key="ServidorSiglaApi" value="https://siglaapifsb.azurewebsites.net/" /> -->
    <add key="ServidorSiglaApi" value="https://siglaapilevumnt.azurewebsites.net" />
    <!-- <add key="ServidorSiglaApi" value="http://localhost/siglawebapi" /> -->
    <!-- <add key="ServidorSiglaApi" value="http://localhost:2420/" /> -->
    <add key="PeriodoScheduller" value="60000" />
    <add key="RodarScheduler" value="S" />
    <add key="ServidorSiglaApiIntegracao" value="https://localhost:7250" />
    <add key="PeriodoIntegracao" value="1800000" />
    <add key="RodarIntegracao" value="S" />
    <add key="ExibirModuloTripulante" value="S" />
    <add key="ExibirEscalaPublicada" value="N" />
    <add key="ExibirConferenciaEscala" value="N" />
    <add key="ExibirQuilometragem" value="N" />
    <!-- <add key="Idioma" value="en-US" /> -->
    <add key="Idioma" value="pt-BR" />
  </appSettings>
  <connectionStrings configSource="SiglaWebConnectionStrings.config" />
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <httpRuntime executionTimeout="600" maxRequestLength="20480" requestValidationMode="2.0" />
    <sessionState timeout="120" />
    <!--
      Brasil ==> 1046 pt-BR
      US ==> 1033 en-US
    -->
    <globalization culture="pt-BR" />
    <!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
    <compilation targetFramework="4.5">
      <assemblies>
        <add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
      </assemblies>
    </compilation>
    <!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
    <authentication mode="Forms">
      <forms loginUrl="~/LoginElmah.aspx" name=".ASPNETAUTH" protection="None" path="/" timeout="20" />
    </authentication>
    <!--
            The <customErrors> section enables configuration 
            of what to do if/when an unhandled error occurs 
            during the execution of a request. Specifically, 
            it enables developers to configure html error pages 
            to be displayed in place of a error stack trace.

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
    <customErrors mode="Off" defaultRedirect="Erro.htm">
      <error statusCode="404" redirect="Erro404.htm" />
    </customErrors>
    <pages controlRenderingCompatibilityVersion="3.5" theme="Basico" clientIDMode="AutoID">
      <controls>
        <add tagPrefix="asp" namespace="System.Web.UI.DataVisualization.Charting" assembly="System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      </controls>
    </pages>
    <httpHandlers>
      <add path="elmah.axd" verb="POST,GET,HEAD" type="Elmah.ErrorLogPageFactory, Elmah" />
      <add path="ChartImg.axd" verb="GET,HEAD,POST" type="System.Web.UI.DataVisualization.Charting.ChartHttpHandler, System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" validate="false" />
    </httpHandlers>
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
    </httpModules>
  </system.web>
  <!-- 
        The system.webServer section is required for running ASP.NET AJAX under Internet
        Information Services 7.0.  It is not necessary for previous version of IIS.
    -->
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <add name="Elmah.ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
    </modules>
    <handlers>
      <remove name="ChartImageHandler" />
      <add name="Elmah" path="elmah.axd" verb="POST,GET,HEAD" type="Elmah.ErrorLogPageFactory, Elmah" preCondition="integratedMode" />
      <add name="ChartImageHandler" preCondition="integratedMode" verb="GET,HEAD,POST" path="ChartImg.axd" type="System.Web.UI.DataVisualization.Charting.ChartHttpHandler, System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="AjaxMin" publicKeyToken="21ef50ce11b5d80f" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.14.5506.26196" newVersion="5.14.5506.26196" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <location path="elmah.axd">
    <system.web>
      <authorization>
        <deny users="?" />
      </authorization>
    </system.web>
  </location>
  <applicationSettings>
    <SIGLA.Web.Properties.Settings>
      <setting name="SIGLA_Web_br_com_airframes_webjet_MeuCTMWSService" serializeAs="String">
        <value>http://webjet.airframes.com.br:8080/v3/loadACRs</value>
      </setting>
    </SIGLA.Web.Properties.Settings>
  </applicationSettings>
</configuration>
