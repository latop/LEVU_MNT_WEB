<% 
Dim objConn, objConn_aux, RS, RS_aux
Dim ls_mes_vet, ll_trip
Dim ls_sql, ls_sqlcidade
Dim ls_siglaempresa, ls_nomeempresa, ls_siglaredempresa
Dim ll_seqvoodia, ll_seqtrecho, ll_page
Dim ls_nomecidadeorig, ls_nomecidadedest, ls_nomepaisorig, ls_nomepaisdest, ls_codcidadeorig, ls_codcidadedest
Dim ldt_dtoper, ls_dtoper, ll_nrvoo, ls_prefixo
Dim ls_funcao_vet, ls_nome_vet, ls_coddac_vet, ls_codcargo_vet, ls_passaporte_vet
Dim ls_identidade_vet, ls_datanascimento_vet
Dim ll_seqcidadeorig, ll_seqcidadedest, ls_codiataorig, ls_codiatadest
Dim ls_extra

ls_extra = Request.QueryString("extra")
ll_seqvoodia = Request.QueryString("seqvoodia")
ll_seqtrecho = Request.QueryString("seqtrecho")

If ll_seqvoodia="" Then ll_seqvoodia="0"
If ll_seqtrecho="" Then ll_seqtrecho="0"

ls_mes_vet = ",JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC"
ls_mes_vet = Split(ls_mes_vet,",")

Set objConn = CreateObject("ADODB.CONNECTION")
objConn.Open(StringConexaoSqlServer)
objConn.Execute "SET DATEFORMAT ymd"

Set objConn_aux = CreateObject("ADODB.CONNECTION")
objConn_aux.Open(StringConexaoSqlServer)
objConn_aux.Execute "SET DATEFORMAT ymd"

Set RS_aux = objConn_aux.Execute( "SELECT * FROM sig_parametros" )
ls_siglaempresa = RS_aux("siglaempresa")
ls_siglaredempresa = RS_aux("siglaredempresa")
ls_nomeempresa = RS_aux("nomeempresa")

ls_sql = 			" SELECT sig_tripcargo.codcargo, sig_tripulante.nomeguerra nomeguerra, sig_tripulante.nome, sig_tripulante.coddac, sig_jornada.textojornada, "
ls_sql = ls_sql & "        sig_tripulante.identidade, sig_tripulante.dtnascimento, "
ls_sql = ls_sql & "        sig_jornada.textojornadaaux, sig_tripulante.senioridade, sig_tripulante.passaporte, sig_cargo.ordem, sig_programacao.funcao, "
ls_sql = ls_sql & "        sig_diariovoo.nrvoo, sig_diariovoo.dtoper, aeroporig.codiata codiataorig, aeroporig.seqaeroporto seqaeroporig, "
ls_sql = ls_sql & "        aeroporig.seqcidade seqcidadeorig, aeropdest.codiata codiatadest, aeropdest.seqaeroporto seqaeropdest, "
ls_sql = ls_sql & "        aeropdest.seqcidade seqcidadedest, sig_aeronave.prefixo, 0 as c_ordemfuncao "
ls_sql = ls_sql & "   FROM sig_tripulante, sig_jornada, sig_programacao, sig_escdiariovoo, sig_diariovoo, sig_diariotrecho, sig_aeroporto aeroporig, "
ls_sql = ls_sql & "        sig_aeroporto aeropdest, sig_tripcargo, sig_cargo, sig_aeronave "
ls_sql = ls_sql & "  WHERE sig_tripulante.seqtripulante = sig_jornada.seqtripulante "
ls_sql = ls_sql & "    AND sig_jornada.seqjornada = sig_programacao.seqjornada "
ls_sql = ls_sql & "    AND sig_programacao.seqvoodiaesc = sig_escdiariovoo.seqvoodiaesc "
ls_sql = ls_sql & "    AND sig_programacao.seqaeroporig = sig_diariotrecho.seqaeroporig "
ls_sql = ls_sql & "    AND sig_programacao.seqaeropdest = sig_diariotrecho.seqaeropdest "
ls_sql = ls_sql & "    AND sig_programacao.seqaeroporig = aeroporig.seqaeroporto "
ls_sql = ls_sql & "    AND sig_programacao.seqaeropdest = aeropdest.seqaeroporto "
ls_sql = ls_sql & "    AND sig_jornada.flgcorrente = 'S' "
ls_sql = ls_sql & "    AND sig_tripcargo.seqtripulante = sig_tripulante.seqtripulante "
ls_sql = ls_sql & "    AND sig_tripcargo.dtinicio <= sig_jornada.dtjornada "
ls_sql = ls_sql & "    AND (sig_tripcargo.dtfim >= sig_jornada.dtjornada OR sig_tripcargo.dtfim is null) "
ls_sql = ls_sql & "    AND sig_cargo.codcargo = sig_tripcargo.codcargo "
ls_sql = ls_sql & "    AND sig_escdiariovoo.dtoper = sig_diariovoo.dtoper "
ls_sql = ls_sql & "    AND sig_escdiariovoo.nrvoo = sig_diariovoo.nrvoo "
ls_sql = ls_sql & "    AND sig_diariotrecho.seqvoodia = sig_diariovoo.seqvoodia "
ls_sql = ls_sql & "    AND sig_diariotrecho.prefixoaeronave = sig_aeronave.prefixored "
ls_sql = ls_sql & "    AND sig_diariovoo.seqvoodia = " & ll_seqvoodia
ls_sql = ls_sql & "    AND sig_diariotrecho.seqtrecho = " & ll_seqtrecho
ls_sql = ls_sql & "    AND ( sig_programacao.funcao IN ('I','C') ) "
If ls_extra = "S" Then
	ls_sql = ls_sql & " AND sig_programacao.funcao IN ('E','J','O') "
ElseIf ls_extra = "N" Then
	ls_sql = ls_sql & " AND ( sig_programacao.funcao IS NULL OR sig_programacao.funcao NOT IN ('E','J','O') ) "
End if
ls_sql = ls_sql & " UNION "
ls_sql = ls_sql & " SELECT sig_tripcargo.codcargo, sig_tripulante.nomeguerra nomeguerra, sig_tripulante.nome, sig_tripulante.coddac, sig_jornada.textojornada, "
ls_sql = ls_sql & "        sig_tripulante.identidade, sig_tripulante.dtnascimento, "
ls_sql = ls_sql & "        sig_jornada.textojornadaaux, sig_tripulante.senioridade, sig_tripulante.passaporte, sig_cargo.ordem, sig_programacao.funcao, "
ls_sql = ls_sql & "        sig_diariovoo.nrvoo, sig_diariovoo.dtoper, aeroporig.codiata codiataorig, aeroporig.seqaeroporto seqaeroporig, "
ls_sql = ls_sql & "        aeroporig.seqcidade seqcidadeorig, aeropdest.codiata codiatadest, aeropdest.seqaeroporto seqaeropdest, "
ls_sql = ls_sql & "        aeropdest.seqcidade seqcidadedest, sig_aeronave.prefixo, 1 as c_ordemfuncao"
ls_sql = ls_sql & "   FROM sig_tripulante, sig_jornada, sig_programacao, sig_escdiariovoo, sig_diariovoo, sig_diariotrecho, sig_aeroporto aeroporig, "
ls_sql = ls_sql & "        sig_aeroporto aeropdest, sig_tripcargo, sig_cargo, sig_aeronave "
ls_sql = ls_sql & "  WHERE sig_tripulante.seqtripulante = sig_jornada.seqtripulante "
ls_sql = ls_sql & "    AND sig_jornada.seqjornada = sig_programacao.seqjornada "
ls_sql = ls_sql & "    AND sig_programacao.seqvoodiaesc = sig_escdiariovoo.seqvoodiaesc "
ls_sql = ls_sql & "    AND sig_programacao.seqaeroporig = sig_diariotrecho.seqaeroporig "
ls_sql = ls_sql & "    AND sig_programacao.seqaeropdest = sig_diariotrecho.seqaeropdest "
ls_sql = ls_sql & "    AND sig_programacao.seqaeroporig = aeroporig.seqaeroporto "
ls_sql = ls_sql & "    AND sig_programacao.seqaeropdest = aeropdest.seqaeroporto "
ls_sql = ls_sql & "    AND sig_jornada.flgcorrente = 'S' "
ls_sql = ls_sql & "    AND sig_tripcargo.seqtripulante = sig_tripulante.seqtripulante "
ls_sql = ls_sql & "    AND sig_tripcargo.dtinicio <= sig_jornada.dtjornada "
ls_sql = ls_sql & "    AND (sig_tripcargo.dtfim >= sig_jornada.dtjornada OR sig_tripcargo.dtfim is null) "
ls_sql = ls_sql & "    AND sig_cargo.codcargo = sig_tripcargo.codcargo "
ls_sql = ls_sql & "    AND sig_escdiariovoo.dtoper = sig_diariovoo.dtoper "
ls_sql = ls_sql & "    AND sig_escdiariovoo.nrvoo = sig_diariovoo.nrvoo "
ls_sql = ls_sql & "    AND sig_diariotrecho.seqvoodia = sig_diariovoo.seqvoodia "
ls_sql = ls_sql & "    AND sig_diariotrecho.prefixoaeronave = sig_aeronave.prefixored "
ls_sql = ls_sql & "    AND sig_diariovoo.seqvoodia = " & ll_seqvoodia
ls_sql = ls_sql & "    AND sig_diariotrecho.seqtrecho = " & ll_seqtrecho
ls_sql = ls_sql & "    AND ( sig_programacao.funcao IS NULL OR sig_programacao.funcao NOT IN ('I','C') ) "
If ls_extra = "S" Then
	ls_sql = ls_sql & " AND sig_programacao.funcao IN ('E','J','O') "
ElseIf ls_extra = "N" Then
	ls_sql = ls_sql & " AND ( sig_programacao.funcao IS NULL OR sig_programacao.funcao NOT IN ('E','J','O') ) "
End if

ls_sql = ls_sql & "  ORDER BY sig_cargo.ordem, c_ordemfuncao, sig_tripulante.senioridade, sig_tripulante.nomeguerra "

Set RS = objConn.Execute( ls_sql )

If NOT RS.EOF Then
	' Recupera Aeroporto/Cidade/Pais Origem
	ll_seqcidadeorig = RS("seqcidadeorig")
	ls_codiataorig = RS("codiataorig")
	ls_nomecidadeorig = ""
	ls_codcidadeorig = ""
	ls_nomepaisorig = ""
	
	If NOT IsNull( ll_seqcidadeorig ) Then
		ls_sqlcidade = 					" SELECT sig_cidade.nomecidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_cidade.codcidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_pais.nomepais "
		ls_sqlcidade = ls_sqlcidade & "   FROM sig_cidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_pais "
		ls_sqlcidade = ls_sqlcidade & "  WHERE sig_cidade.codpais = sig_pais.codpais AND "
		ls_sqlcidade = ls_sqlcidade & "        sig_cidade.seqcidade = " & ll_seqcidadeorig
		
		Set RS_aux = objConn_aux.Execute( ls_sqlcidade )
		
		If NOT RS_aux.EOF Then
			ls_nomecidadeorig = RS_aux("nomecidade")
			ls_codcidadeorig = RS_aux("codcidade")
			ls_nomepaisorig = RS_aux("nomepais")
		End if
	End if
		
	' Recupera Aeroporto/Cidade/Pais Destino
	ll_seqcidadedest = RS("seqcidadedest")
	ls_codiatadest = RS("codiatadest")
	ls_nomecidadedest = ""
	ls_codcidadedest = ""
	ls_nomepaisdest = ""
	
	If NOT IsNull( ll_seqcidadedest ) Then
		ls_sqlcidade = 					" SELECT sig_cidade.nomecidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_cidade.codcidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_pais.nomepais "
		ls_sqlcidade = ls_sqlcidade & "   FROM sig_cidade, "
		ls_sqlcidade = ls_sqlcidade & "        sig_pais "
		ls_sqlcidade = ls_sqlcidade & "  WHERE sig_cidade.codpais = sig_pais.codpais AND "
		ls_sqlcidade = ls_sqlcidade & "        sig_cidade.seqcidade = " & ll_seqcidadedest
		
		Set RS_aux = objConn_aux.Execute( ls_sqlcidade )
		
		If NOT RS_aux.EOF Then
			ls_nomecidadedest = RS_aux("nomecidade")
			ls_codcidadedest = RS_aux("codcidade")
			ls_nomepaisdest = RS_aux("nomepais")
		End if
	End if
	
	' Recupera o Numero do Voo
	ll_nrvoo = RS("nrvoo")
	
	' Recupera Prefixo Aeronave
	ls_prefixo = RS("prefixo")
	
	' Recupera a Data do Voo
	ldt_dtoper = RS("dtoper")
	ls_dtoper = Right("00"&Day(ldt_dtoper),2) & ls_mes_vet(Month(ldt_dtoper)) & Year(ldt_dtoper)
End if

ls_funcao_vet = ""
ls_nome_vet = ""
ls_coddac_vet = ""
ls_codcargo_vet = ""
ls_passaporte_vet = ""
ls_identidade_vet = ""
ls_datanascimento_vet = ""

Do While NOT RS.EOF
	ls_funcao_vet = ls_funcao_vet & "," & RS("funcao")
	ls_nome_vet = ls_nome_vet & "," & RS("nome")
	ls_coddac_vet = ls_coddac_vet & "," & RS("coddac")
	ls_codcargo_vet = ls_codcargo_vet & "," & RS("codcargo")
	ls_passaporte_vet = ls_passaporte_vet & "," & RS("passaporte")
	ls_identidade_vet = ls_identidade_vet & "," & RS("identidade")
	ls_datanascimento_vet = ls_datanascimento_vet & "," & RS("dtnascimento")
	RS.MoveNext
LOOP

objConn.close
objConn_aux.close

ls_funcao_vet = Split( ls_funcao_vet, "," )
ls_nome_vet = Split( ls_nome_vet, "," )
ls_coddac_vet = Split( ls_coddac_vet, "," )
ls_codcargo_vet = Split( ls_codcargo_vet, "," )
ls_passaporte_vet = Split( ls_passaporte_vet, "," )
ls_identidade_vet = Split( ls_identidade_vet, "," )
ls_datanascimento_vet = Split( ls_datanascimento_vet, "," )

ll_page = 1

Do While ll_page = 1 OR ( ll_page = 2 AND UBound( ls_nome_vet ) ) > 20

	If ll_page = 2 Then
		Response.Write("<p style=&quot;page-break-before:always&quot;>&nbsp;</p>")
	End if
%>

